{
  "family": "ofcrm-frontend",
  "taskRoleArn": "",
  "executionRoleArn": "",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "ofcrm-frontend",
      "image": "",
      "cpu": 256,
      "memory": 512,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 3000,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "NEXT_PUBLIC_APP_ENV",
          "value": "production"
        },
        {
          "name": "NEXT_PUBLIC_API_URL",
          "value": "https://api.tkb-tech.com"
        },
        {
          "name": "NEXT_PUBLIC_PUSHER_APP_KEY",
          "value": ""
        },
        {
          "name": "NEXT_PUBLIC_PUSHER_HOST",
          "value": ""
        },
        {
          "name": "NEXT_PUBLIC_PUSHER_PORT",
          "value": "443"
        },
        {
          "name": "NEXT_PUBLIC_PUSHER_SCHEME",
          "value": "https"
        },
        {
          "name": "NEXT_PUBLIC_PUSHER_APP_CLUSTER",
          "value": "ap3"
        }
      ],
      "mountPoints": [],
      "volumesFrom": [],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/ofcrm-frontend",
          "awslogs-region": "ap-northeast-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "systemControls": []
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}
